{
  "name": "browserify-tutorial",
  "version": "1.0.0",
  "description": "Boiler plate for AngularJS using Browserify",
  "main": "index.js",
  "scripts": {
    "build": "browserify app.js -o bundle.js",
    "serve": "concurrently \"watchify app/js/app.module.js -o bundle.js\" \"browser-sync start --server -w --files './app/js/*.js'\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.6.8",
    "angular-animate": "^1.6.8",
    "angular-aria": "^1.6.8",
    "angular-material": "^1.1.5",
    "angular-messages": "^1.6.8",
    "angular-route": "^1.6.8"
  },
  "devDependencies": {
    "browser-sync": "^2.23.5",
    "browserify": "^15.1.0",
    "concurrently": "^3.5.1",
    "watchify": "^3.9.0"
  }
}
